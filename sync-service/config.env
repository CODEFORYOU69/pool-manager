# Connexion à la base de données locale (source)
DATABASE_URL="postgresql://postgres:postgres@localhost:5432/taekwondo_db"

# Connexion à la base de données distante (destination)
# Remplacer avec vos informations de connexion réelles lors du déploiement
REMOTE_DATABASE_URL="postgresql://postgres:postgres@remote-db-server:5432/taekwondo_remote_db"

# Intervalle de synchronisation en secondes
SYNC_INTERVAL=5 